{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "default",
  "description": "Default capabilities for the app",
  "windows": ["main"],
  "remote": {
    "urls": ["http://127.0.0.1:*", "http://localhost:*"]
  },
  "permissions": [
    "core:default",
    "shell:allow-spawn",
    "shell:allow-execute",
    "shell:allow-kill",
    "process:default",
    "updater:default",
    "updater:allow-check",
    "updater:allow-download-and-install"
  ]
}
